@model dynamic

@{
    ViewBag.Title = "Password Reset Requested";
    Layout = "_Layout";
}

<h2>Password Reset Requested</h2>

<div class="mt-4">
    <p>
        If an account with the email you provided exists, a password reset link has been sent.
        Please check your email to reset your password.
    </p>
    <a asp-action="Login" class="btn tbn-primary mt-2">Back to Login</a>
</div>
